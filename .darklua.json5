{
  generator: "retain_lines",

  bundle: {
    modules_identifier: "__DARKLUA_BUNDLE_MODULES",
  	excludes: [ "@lune/**" ],
    require_mode: {
      name: "luau",
      use_luau_configuration: false,
    },
  },

  rules: [
    "remove_comments",
    "remove_spaces",
    "remove_nil_declaration",
    "compute_expression",
    "remove_unused_if_branch",
    "filter_after_early_return",
    "remove_empty_do",
  ],
}